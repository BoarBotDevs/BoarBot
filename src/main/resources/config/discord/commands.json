{
  "main": {
    "name": "boar",
    "description": "Commands to play and interact with BoarBot",
    "subcommands": {
      "help": {
        "name": "help",
        "description": "Learn more about how the bot works",
        "location": "dev.boarbot.commands.boar.HelpSubcommand",
        "options": [
          {
            "name": "menu",
            "type": 3,
            "description": "The help menu you want to view",
            "choices": [
              {
                "name": "General",
                "value": "general"
              },
              {
                "name": "Boars",
                "value": "boars"
              },
              {
                "name": "Badges",
                "value": "badges"
              },
              {
                "name": "Powerups",
                "value": "powerups"
              }
            ]
          },
          {
            "name": "page",
            "type": 4,
            "description": "The page to start out on"
          }
        ]
      },
      "daily": {
        "name": "daily",
        "description": "Get your daily boar",
        "location": "dev.boarbot.commands.boar.DailySubcommand",
        "options": [
          {
            "name": "powerup",
            "type": 3,
            "description": "Choose which powerups to use",
            "choices": [
              {
                "name": "I want to use a powerup!",
                "value": "y"
              }
            ]
          }
        ]
      },
      "profile": {
        "name": "profile",
        "description": "View a user's general profile information",
        "location": "dev.boarbot.commands.boar.megamenu.ProfileSubcommand",
        "options": [
          {
            "name": "user",
            "type": 6,
            "description": "Check another user's profile"
          }
        ]
      },
      "collection": {
        "name": "collection",
        "description": "View a user's unique boars",
        "location": "dev.boarbot.commands.boar.megamenu.CollectionSubcommand",
        "options": [
          {
            "name": "user",
            "type": 6,
            "description": "View another user's collection"
          },
          {
            "name": "page",
            "type": 4,
            "description": "Page to start off on"
          }
        ]
      },
      "compendium": {
        "name": "compendium",
        "description": "View a user's compendium of boars",
        "location": "dev.boarbot.commands.boar.megamenu.CompendiumSubcommand",
        "options": [
          {
            "name": "user",
            "type": 6,
            "description": "View another user's compendium"
          },
          {
            "name": "search",
            "type": 3,
            "description": "Search term for a boar"
          }
        ]
      },
      "stats": {
        "name": "stats",
        "description": "View a user's statistics",
        "location": "dev.boarbot.commands.boar.megamenu.StatsSubcommand",
        "options": [
          {
            "name": "user",
            "type": 6,
            "description": "View another user's statistics"
          },
          {
            "name": "page",
            "type": 4,
            "description": "Page to start off on"
          }
        ]
      },
      "powerups": {
        "name": "powerups",
        "description": "Use/View a user's powerups",
        "location": "dev.boarbot.commands.boar.megamenu.PowerupsSubcommand",
        "options": [
          {
            "name": "user",
            "type": 6,
            "description": "View another user's powerups"
          }
        ]
      },
      "quests": {
        "name": "quests",
        "description": "View a user's progress on weekly quests",
        "location": "dev.boarbot.commands.boar.megamenu.QuestsSubcommand",
        "options": [
          {
            "name": "user",
            "type": 6,
            "description": "View another user's weekly quests"
          }
        ]
      },
      "badges": {
        "name": "badges",
        "description": "View a user's badges",
        "location": "dev.boarbot.commands.boar.megamenu.BadgesSubcommand",
        "options": [
          {
            "name": "user",
            "type": 6,
            "description": "View another user's badges"
          },
          {
            "name": "page",
            "type": 4,
            "description": "Page to start off on"
          }
        ]
      },
      "advent": {
        "name": "advent",
        "description": "View a user's advent calendar",
        "location": "dev.boarbot.commands.boar.megamenu.AdventSubcommand",
        "options": [
          {
            "name": "user",
            "type": 6,
            "description": "View another user's advent calendar"
          }
        ]
      },
      "leaderboard": {
        "name": "leaderboard",
        "description": "View leaderboards showcasing a ranking of player stats",
        "location": "dev.boarbot.commands.boar.TopSubcommand",
        "options": [
          {
            "name": "board",
            "type": 3,
            "description": "The leaderboard to view",
            "choices": [
              {
                "name": "Boar Bucks",
                "value": "total_bucks"
              },
              {
                "name": "Total Boars",
                "value": "total_boars"
              },
              {
                "name": "Unique Boars",
                "value": "unique_boars"
              },
              {
                "name": "Boar Streak",
                "value": "boar_streak"
              },
              {
                "name": "Number of Perfect Powerups",
                "value": "powerup_perfects"
              },
              {
                "name": "Gifts Used",
                "value": "gift"
              },
              {
                "name": "Base Boar Blessings",
                "value": "blessings"
              },
              {
                "name": "Peak Boar Blessings",
                "value": "highest_blessings"
              },
              {
                "name": "Fastest Powerup Event",
                "value": "powerup_fastest_time"
              },
              {
                "name": "Transmutation Charges Used",
                "value": "transmute"
              }
            ]
          },
          {
            "name": "user",
            "type": 6,
            "description": "The user to find on the leaderboard"
          },
          {
            "name": "page",
            "type": 4,
            "description": "The page to start out on"
          }
        ]
      },
      "market": {
        "name": "market",
        "description": "Buy and sell boars/items",
        "location": "dev.boarbot.commands.boar.MarketSubcommand",
        "options": [
          {
            "name": "search",
            "type": 3,
            "description": "The item you want to buy/sell"
          }
        ]
      },
      "report": {
        "name": "report",
        "description": "Report a bug/exploit with BoarBot",
        "location": "dev.boarbot.commands.boar.ReportSubcommand"
      },
      "self-wipe": {
        "name": "self-wipe",
        "description": "Wipe your profile clean. Deletes all stored data tied to your account",
        "location": "dev.boarbot.commands.boar.SelfWipeSubcommand"
      },
      "gift": {
        "name": "gift",
        "description": "Send a Boar Gift in the channel you're currently in",
        "location": "dev.boarbot.commands.boar.GiftSubcommand"
      },
      "support": {
        "name": "support",
        "description": "Get the link to BoarBot's Patreon",
        "location": "dev.boarbot.commands.boar.SupportSubcommand"
      },
      "guess": {
        "name": "guess",
        "description": "Guess a value for hunts",
        "location": "dev.boarbot.commands.boar.GuessSubcommand",
        "options": [
          {
            "name": "value",
            "type": 3,
            "description": "Value to guess",
            "required": true
          }
        ]
      }
    }
  },
  "manage": {
    "name": "boar-manage",
    "default_member_permissions": 32,
    "description": "Commands to manage BoarBot",
    "subcommands": {
      "setup": {
        "name": "setup",
        "description": "Set BoarBot up to your liking",
        "location": "dev.boarbot.commands.boarmanage.SetupSubcommand"
      }
    }
  },
  "dev": {
    "name": "boar-dev",
    "default_member_permissions": 0,
    "description": "Commands exclusive to developers of BoarBot",
    "subcommands": {
      "give": {
        "name": "give",
        "description": "Give a user a boar/badge",
        "location": "dev.boarbot.commands.boardev.GiveSubcommand",
        "options": [
          {
            "name": "user",
            "type": 6,
            "description": "User you'd like to give the item to",
            "required": true
          },
          {
            "name": "type",
            "type": 4,
            "description": "Type of item to give",
            "required": true,
            "choices": [
              {
                "name": "Boar",
                "value": 0
              },
              {
                "name": "Badge",
                "value": 1
              },
              {
                "name": "Powerup",
                "value": 2
              },
              {
                "name": "Bucks",
                "value": 3
              }
            ]
          },
          {
            "name": "id",
            "type": 3,
            "description": "ID of item to give"
          },
          {
            "name": "tier",
            "type": 4,
            "description": "Tier of item to give (if it's a badge)"
          },
          {
            "name": "amount",
            "type": 4,
            "description": "Amount of item to give"
          }
        ]
      },
      "ban": {
        "name": "ban",
        "description": "Ban a user from community BoarBot features",
        "location": "dev.boarbot.commands.boardev.BanSubcommand",
        "options": [
          {
            "name": "user",
            "type": 6,
            "description": "User to ban",
            "required": true
          },
          {
            "name": "duration",
            "type": 4,
            "description": "Duration of ban in hours (blank for unban)"
          }
        ]
      },
      "maintenance": {
        "name": "maintenance",
        "description": "Set BoarBot's maintenance status",
        "location": "dev.boarbot.commands.boardev.MaintenanceSubcommand",
        "options": [
          {
            "name": "value",
            "type": 5,
            "description": "Maintenance value",
            "required": true
          }
        ]
      },
      "freeze": {
        "name": "freeze",
        "description": "Set every user's streak freeze status",
        "location": "dev.boarbot.commands.boardev.FreezeSubcommand",
        "options": [
          {
            "name": "value",
            "type": 5,
            "description": "Freeze value",
            "required": true
          }
        ]
      }
    }
  }
}